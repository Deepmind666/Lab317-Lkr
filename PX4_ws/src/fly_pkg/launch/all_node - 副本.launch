<launch>
    <!-- This node will launch MAVROS -->
    <include file="$(find mavros)/launch/px4.launch"/>

    <!-- This node will launch the node that bridges realsense-ros and MAVROS -->
    <include file="$(find vision_to_mavros)/launch/t265_tf_to_mavros.launch"/>

    <!-- This node will launch the node that bridges t265 and get_light -->
    <include file="$(find fly_pkg)/launch/light.launch"/>

    <include file="$(find fly_pkg)/launch/pos.launch"/>

    <include file="$(find fly_pkg)/launch/main.launch"/>
   
</launch>